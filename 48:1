#include <stdio.h>
#include <string.h>

int main() {
    char str1[100], str2[100];
    scanf("%[^\n]", str1);
    getchar();
    scanf("%[^\n]", str2);

    int len1 = 0, len2 = 0;
    while(str1[len1] != '\0') len1++;
    while(str2[len2] != '\0') len2++;

    if(len1 != len2) {
        printf("Not Rotation");
        return 0;
    }

    char temp[200];
    for(int i = 0; i < len1; i++)
        temp[i] = str1[i];
    for(int i = 0; i < len1; i++)
        temp[len1+i] = str1[i];
    temp[2*len1] = '\0';

    int found = 0;
    for(int i = 0; i <= len1; i++) {
        int j;
        for(j = 0; j < len2; j++) {
            if(temp[i+j] != str2[j]) break;
        }
        if(j == len2) {
            found = 1;
            break;
        }
    }

    if(found) printf("Rotation");
    else printf("Not Rotation");

    return 0;
}
