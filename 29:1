#include <stdio.h>

int main() {
    int n, i;
    int a[100];
    int sum = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("Enter element %d: ", i+1);
        scanf("%d", &a[i]);
    }

    for(i = 0; i < n; i++) {
        sum += a[i];
    }

    printf("Sum of array elements = %d", sum);

    return 0;
}
