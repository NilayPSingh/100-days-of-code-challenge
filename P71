#include <stdio.h>
#include <string.h>

int main(){
    char name[256];
    int age;
    getchar();
    if(!fgets(name,sizeof(name),stdin)) return 0;
    size_t ln=strlen(name);
    if(ln>0 && name[ln-1]=='\n') name[ln-1]=0;
    if(scanf("%d",&age)!=1) return 0;
    FILE *f=fopen("info.txt","w");
    if(!f) return 0;
    fprintf(f,"%s\n%d\n",name,age);
    fclose(f);
    printf("Data saved successfully\n");
    return 0;
}
