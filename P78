#include <stdio.h>
#include <ctype.h>

int main(){
    char fname[256];
    if(scanf("%s",fname)!=1) return 0;
    FILE *f=fopen(fname,"r");
    if(!f){ printf("Cannot open file\n"); return 0; }
    long long vowels=0,consonants=0;
    int c;
    while((c=fgetc(f))!=EOF){
        if(isalpha(c)){
            char ch=tolower(c);
            if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u') vowels++; else consonants++;
        }
    }
    fclose(f);
    printf("%lld %lld\n",vowels,consonants);
    return 0;
}
