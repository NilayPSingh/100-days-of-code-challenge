#include <stdio.h>
#include <string.h>

int main(){
    char fname[256];
    getchar();
    if(fgets(fname,sizeof(fname),stdin)==NULL) return 0;
    size_t ln=strlen(fname);
    if(ln>0 && fname[ln-1]=='\n') fname[ln-1]=0;
    char line[1005];
    if(!fgets(line,sizeof(line),stdin)) return 0;
    ln=strlen(line);
    if(ln>0 && line[ln-1]=='\n') line[ln-1]=0;
    FILE *f=fopen(fname,"a");
    if(!f){ printf("Cannot open file\n"); return 0; }
    fprintf(f,"%s\n",line);
    fclose(f);
    printf("Appended\n");
    return 0;
}
