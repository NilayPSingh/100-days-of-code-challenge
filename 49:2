#include <stdio.h>
#include <string.h>

int main() {
    char name[100];
    scanf("%[^\n]", name);

    int i = 0;
    while(name[i] != ' ' && name[i] != '\0') {
        printf("%c. ", name[i]);
        i++;
    }

    while(name[i] == ' ') i++; // skip spaces
    printf("%s", &name[i]);

    return 0;
}
