#include <stdio.h>

enum Gender{MALE,FEMALE,OTHER};

struct Person{ char name[100]; enum Gender g; };

int main(){
    struct Person p;
    scanf("%99s %d",p.name,(int*)&p.g);
    if(p.g==MALE) printf("%s Male\n",p.name);
    else if(p.g==FEMALE) printf("%s Female\n",p.name);
    else printf("%s Other\n",p.name);
    return 0;
}
