#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int main(){
    int n;
    if(scanf("%d",&n)!=1) return 0;
    int *a=(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    long long max_ending=LLONG_MIN, max_so_far=LLONG_MIN;
    for(int i=0;i<n;i++){
        if(max_ending==LLONG_MIN) max_ending=a[i]; else {
            if(max_ending<0) max_ending=a[i]; else max_ending += a[i];
        }
        if(max_ending>max_so_far) max_so_far=max_ending;
    }
    printf("%lld\n",max_so_far);
    free(a);
    return 0;
}
