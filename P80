#include <stdio.h>

int main(){
    int n;
    if(scanf("%d",&n)!=1) return 0;
    FILE *f=fopen("students.txt","w");
    if(!f) return 0;
    for(int i=0;i<n;i++){
        char name[100];
        int roll,marks;
        getchar();
        fgets(name,sizeof(name),stdin);
        size_t ln=strlen(name);
        if(ln>0 && name[ln-1]=='\n') name[ln-1]=0;
        scanf("%d %d",&roll,&marks);
        fprintf(f,"%s %d %d\n",name,roll,marks);
    }
    fclose(f);
    f=fopen("students.txt","r");
    if(!f) return 0;
    char name[100];
    int roll,marks;
    while(fscanf(f,"%99s %d %d",name,&roll,&marks)==3){
        printf("%s %d %d\n",name,roll,marks);
    }
    fclose(f);
    return 0;
}
