#include <stdio.h>
#include <stdlib.h>

int main(){
    int n;
    if(scanf("%d",&n)!=1) return 0;
    int *a=(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int xor_all=0;
    for(int i=0;i<=n;i++) xor_all ^= i;
    for(int i=0;i<n;i++) xor_all ^= a[i];
    printf("%d\n",xor_all);
    free(a);
    return 0;
}
