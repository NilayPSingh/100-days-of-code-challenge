#include <stdio.h>

int main() {
    int num, digit, i, maxDigit = 0;
    int count[10] = {0};

    printf("Enter an integer: ");
    scanf("%d", &num);
    if(num < 0) num = -num;

    while(num > 0) {
        digit = num % 10;
        count[digit]++;
        num /= 10;
    }

    int maxCount = count[0];
    for(i = 1; i < 10; i++) {
        if(count[i] > maxCount) {
            maxCount = count[i];
            maxDigit = i;
        }
    }

    printf("Digit that occurs most: %d\n", maxDigit);
    return 0;
}
