#include <stdio.h>
#include <stdlib.h>

int main(){
    int n;
    if(scanf("%d",&n)!=1) return 0;
    int *a=(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int target; scanf("%d",&target);
    int i1=-1,i2=-1;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(a[i]+a[j]==target){ i1=i; i2=j; break; }
        }
        if(i1!=-1) break;
    }
    if(i1==-1) printf("-1 -1\n"); else printf("%d %d\n",i1,i2);
    free(a);
    return 0;
}
