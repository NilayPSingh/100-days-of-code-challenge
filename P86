#include <stdio.h>

enum Choice{ADD,SUBTRACT,MULTIPLY};

int main(){
    int x,y;
    if(scanf("%d %d",&x,&y)!=2) return 0;
    int ch;
    if(scanf("%d",&ch)!=1) return 0;
    enum Choice c=ch;
    int res=0;
    switch(c){
        case ADD: res=x+y; break;
        case SUBTRACT: res=x-y; break;
        case MULTIPLY: res=x*y; break;
        default: printf("Invalid\n"); return 0;
    }
    printf("%d\n",res);
    return 0;
}
