#include <stdio.h>
#include <string.h>

int main() {
    char str[200];
    scanf("%[^\n]", str);

    int start = 0, i = 0;
    while(1) {
        if(str[i] == ' ' || str[i] == '\0') {
            for(int j = i-1; j >= start; j--)
                printf("%c", str[j]);
            if(str[i] == ' ') printf(" ");
            start = i + 1;
        }
        if(str[i] == '\0') break;
        i++;
    }

    return 0;
}
